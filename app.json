{
    "expo": {
        "name": "game-shelf",
        "slug": "game-shelf",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#3B3940"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.gameshelf.gameshelf",
            "icon": "./assets/icon.png"
        },
        "android": {
            "package": "com.gameshelf.gameshelf",
            "icon": "./assets/icon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptative-icon.png",
                "backgroundColor": "#3B3940"
            }
        }
    }
}
